        <mah:WindowCommands>
            <Button Click="LaunchTelegram" ToolTip="Связь с автором" Content="{iconPacks:FontAwesome TelegramPlaneBrands, Height=22, Width=22}"/>
            <Button Click="DevChannel" ToolTip="Открыть канал разработчика" Content="{iconPacks:FontAwesome TelegramPlaneBrands, Height=22, Width=22}"/>
            <Button Click="UpdateCheck" ToolTip="Проверить обновления программы" Content="{iconPacks:FontAwesome CloudDownloadAltSolid, Height=22, Width=22}"/>
            <Button Click="opensettings" ToolTip="Открыть настройки приложения">
                <iconPacks:FontAwesome Width="22" Height="22" Kind="UserCogSolid" />
            </Button>
        </mah:WindowCommands>
    </mah:MetroWindow.LeftWindowCommands>
    <mah:MetroWindow.RightWindowCommands>
        <mah:WindowCommands>
            <Button x:Name="buttonright" Click="opennewwindows" ToolTip="Открыть во внешнем плеере" Content="{iconPacks:FontAwesome VolumeUpSolid, Height=22, Width=22}" />
